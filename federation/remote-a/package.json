{
  "name": "@federation/remote-a",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "NODE_ENV=production webpack --config internal/webpack.config.js",
    "lint": "eslint .",
    "serve": "serve dist -s -p 3001",
    "clean": "rm -rf dist",
    "docker-build": "docker build --no-cache --rm -f internal/Dockerfile -t codepunkt/federation/remote-a .",
    "docker-run": "docker run --rm -d -p 3101:80/tcp --name federation-remote-a codepunkt/federation/remote-a:latest",
    "start": "NODE_ENV=development webpack-dev-server --config internal/webpack.config.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
